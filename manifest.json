{
  "name": "guld",
  "short_name": "guld",
  "version": "0.3.1",
  "description": "guld browser extension",
  "key": "",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "activeTab",
    "declarativeContent", 
    "storage",
    "unlimitedStorage", 
    "nativeMessaging", 
    "identity",
    "management",
    "https://github.com/",
    "https://api.github.com/"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+G",
        "mac": "Alt+Shift+G",
        "chromeos": "Alt+Shift+G",
        "linux": "Alt+Shift+G"
      }
    }
  },
  "browser_action": {
    "default_title": "guld",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "oauth2": {
    "client_id": "e0e419687209f5812b69",
    "scopes": [
      "repo",
      "admin:repo_hook",
      "user",
      "admin:gpg_key",
      "read:public_key"
    ]
  },
  "background": {
    "scripts": [
      "js/vendor/EventEmitter.js",
      "js/vendor/GitHub.bundle.js",
      "js/vendor/browserfs.min.js",
      "js/vendor/isomorphic-git.min.js",
      "js/vendor/openpgp.min.js",
      "js/vendor/isomorphic-git.openpgp.js",
      "js/vendor/decimal.min.js",
      "js/vendor/highland.js",
      "js/vendor/papaparse.js",
      "js/vendor/ledger.types.js",
      "js/vendor/ledger.js",
      "js/vendor/guld-lib.js",
      "js/background.js"
    ]
  },
  "options_page": "html/options.html",
  "manifest_version": 2
}
